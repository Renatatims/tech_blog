
<div >
    <h2>Welcome, {{username}}!</h2>
<div>

<div>
  <h3> Create New Post </h3>

<form class="new-post-form">
  <div class="form-group">
    <label for="exampleInputEmail1">Title</label>
    <input class="form-control" id="titleNewPost" name = "titleNewPost" aria-describedby="emailHelp" placeholder="Enter Title">
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Content</label>
    <input class="form-control" id="contentNewPost" name="contentNewPost" placeholder="Content">
  </div>
  
  <button type="submit" class="btn btn-primary" id="postBtn">Post</button>
</form>

</div>

{{#if posts.length}}
<div>
    <h3> Post History </h3>
<div>
     {{#each posts as |post| }}
        {{> postData post}}
    {{/each}}

</div>
 {{/if}}

 <script src="./js/dashboard.js"></script>